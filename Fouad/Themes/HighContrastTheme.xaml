<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- High Contrast Theme Color Overrides -->
    <Color x:Key="BackgroundColor">#000000</Color>
    <Color x:Key="ForegroundColor">#FFFFFF</Color>
    <Color x:Key="BorderColor">#FFFFFF</Color>
    <Color x:Key="HoverColor">#333333</Color>
    <Color x:Key="PressedColor">#555555</Color>
    
    <!-- High Contrast Theme Brushes -->
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderColor}"/>
    <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource HoverColor}"/>
    <SolidColorBrush x:Key="PressedBrush" Color="{StaticResource PressedColor}"/>
    
    <!-- High Contrast Theme-specific overrides -->
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="#FFFF00"/> <!-- Yellow for high contrast -->
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="#00FFFF"/> <!-- Cyan for high contrast -->
    
</ResourceDictionary>